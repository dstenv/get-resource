
.container {
	width: 100vw;
	height: 100vh;
}
.box {
	box-sizing: border-box;
	padding: 20rpx 40rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
}
.box button {
	margin-left: 10rpx;
	font-size: 16px;
}
.input {
	display: block;
	border: 1rpx solid dodgerblue;
	border-radius: 10rpx;
	padding: 10rpx;
	flex: 1;
}
.img-list {
	display: flex;
	align-items: center;
	justify-content: space-around;
	gap: 20rpx 0;
	flex-wrap: wrap;
}
.origin-box {
	overflow: hidden;
	border-radius: 10rpx;
}
.origin-url {
	border-radius: 10rpx;
	width: 300rpx;
	display: block;
}
